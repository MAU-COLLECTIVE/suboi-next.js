{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ], 
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "rules": {    
            "import/no-unresolved": [       
                2,       
                { 
                    "caseSensitive": false 
                }      
            ]
        }
    }
}  