{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "app",
    "paths": {
      "@element/*": ["components/elements/*"],
      "@styles/*": ["components/styles/*"],
      "@layout/*": ["components/layouts/*"],
      "@module/*": ["components/modules/*"],
      "@template/*": ["components/templates/*"],
      "@constant/*": ["constants/*"],
      "@context/*": ["context/*"],
      "@hook/*": ["hooks/*"],
      "@util/*": ["utils/*"]
    }
  }
}